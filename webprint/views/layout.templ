package views

templ Layout() {
	<!DOCTYPE html>
	<html>
		<head>
			<meta charset="utf-8"/>
			<script src="/static/wasm/wasm_exec.js"></script>
			<script>
				const go = new Go();
				WebAssembly.instantiateStreaming(fetch("/static/wasm/main.wasm"), go.importObject).then((result) => {
					go.run(result.instance);
				});
			</script>
		</head>
		<body>
			{ children... }
		</body>
	</html>
}
